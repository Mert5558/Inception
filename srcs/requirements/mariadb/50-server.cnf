
[server]

[mysqld]

user                    = root 
pid-file                = /run/mysqld/mysqld.pid
basedir                 = /usr
datadir                 = /var/lib/mysql
tmpdir                  = /tmp

skip-networking = 0
skip-name-resolve

bind-address            = 0.0.0.0

socket = /var/run/mysqld/mysqld.sock

character-set-server  = utf8mb4
collation-server      = utf8mb4_general_ci

[embedded]

[mariadb]

[mariadb-10.11]
